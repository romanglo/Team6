<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane fx:id="anchorPane_mainStage" minHeight="445.0"
	minWidth="445.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="445.0"
	prefWidth="560.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controllers.ShopManagerController">
	<children>

		<AnchorPane fx:id="anchorPane_viewStage" layoutY="95.0"
			prefHeight="345.0" prefWidth="435.0" AnchorPane.bottomAnchor="5.0"
			AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0"
			AnchorPane.topAnchor="95.0">
			<children>
				<ComboBox fx:id="comboBox_selectionReportType" layoutX="187.0"
					layoutY="15.0" onAction="#reportTypeChanged" prefHeight="25.0"
					prefWidth="175.0" AnchorPane.topAnchor="15.0" />
				<ComboBox fx:id="comboBox_selectionQuarter" layoutX="12.0"
					layoutY="15.0" prefHeight="25.0" prefWidth="93.0"
					AnchorPane.leftAnchor="12.0" AnchorPane.topAnchor="15.0" />
				<TextField fx:id="textField_selectionYear" layoutX="112.0"
					layoutY="15.0" prefHeight="25.0" prefWidth="69.0"
					AnchorPane.topAnchor="15.0" />
				<Button fx:id="button_submit" layoutX="466.0" layoutY="15.0"
					minHeight="25.0" mnemonicParsing="false" onAction="#showSelectionReport"
					prefHeight="25.0" prefWidth="84.0" text="Show Report" textOverrun="CLIP"
					AnchorPane.topAnchor="15.0" />
				<Pane fx:id="pane_dataPane" layoutX="14.0" layoutY="48.0"
					prefHeight="274.0" prefWidth="405.0" AnchorPane.bottomAnchor="35.0"
					AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0"
					AnchorPane.topAnchor="50.0" />
				<Button fx:id="button_compare" layoutX="15.0" layoutY="319.0"
					mnemonicParsing="false" onAction="#compareReports" text="Compare Reports"
					AnchorPane.bottomAnchor="3.0" AnchorPane.leftAnchor="15.0" />
				<ComboBox fx:id="comboBox_selectionStore" layoutX="369.0"
					layoutY="15.0" prefHeight="25.0" prefWidth="93.0"
					AnchorPane.topAnchor="15.0" />
			</children>
		</AnchorPane>

		<!-- Title: -->
		<ImageView fx:id="imageview_gif" fitHeight="84.0" fitWidth="59.0"
			layoutX="336.0" layoutY="6.0" pickOnBounds="true" preserveRatio="true"
			AnchorPane.rightAnchor="16.0" AnchorPane.topAnchor="6.0" />
		<ImageView fx:id="imageview_title" fitHeight="48.0"
			fitWidth="154.0" layoutX="14.0" layoutY="6.0" pickOnBounds="true"
			AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="6.0" />
	</children>
</AnchorPane>
