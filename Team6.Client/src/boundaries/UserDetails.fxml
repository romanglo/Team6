<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="355.0" maxWidth="300.0" minHeight="355"
	minWidth="300.0" pickOnBounds="true" prefHeight="355" prefWidth="300.0"
	styleClass="user-details" xmlns="http://javafx.com/javafx/8.0.152"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.UserDetailsController">
	<children>
		<Rectangle arcHeight="5.0" arcWidth="5.0" fill="TRANSPARENT"
			height="355.0" pickOnBounds="true" stroke="TEAL" strokeType="INSIDE"
			strokeWidth="3" width="300.0" AnchorPane.bottomAnchor="5.0"
			AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0"
			AnchorPane.topAnchor="5.0" />
		<Rectangle arcHeight="5.0" arcWidth="5.0" fill="TRANSPARENT"
			height="344.0" stroke="TEAL" strokeType="INSIDE" strokeWidth="3"
			width="290.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0"
			AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0" />
		<VBox alignment="TOP_CENTER" layoutX="14.0" layoutY="14.0"
			prefHeight="336.0" prefWidth="281.0">
			<children>
				<ImageView fx:id="imageview_user" fitHeight="150.0"
					fitWidth="150.0" preserveRatio="true">
					<VBox.margin>
						<Insets bottom="10.0" />
					</VBox.margin>
				</ImageView>
				<HBox alignment="TOP_CENTER" prefWidth="200.0">
					<children>
						<Label alignment="TOP_CENTER" text="Username:">
							<HBox.margin>
								<Insets bottom="10.0" right="3" />
							</HBox.margin>
							<font>
								<Font name="Arial Bold" size="16.0" />
							</font>
						</Label>
						<Label fx:id="label_username">
							<font>
								<Font name="Arial" size="16.0" />
							</font>
						</Label>
					</children>
				</HBox>
				<HBox alignment="TOP_CENTER" layoutX="10.0" layoutY="160.0">
					<children>
						<Label alignment="TOP_CENTER" text="Password:">
							<HBox.margin>
								<Insets bottom="10.0" right="3" />
							</HBox.margin>
							<font>
								<Font name="Arial Bold" size="16.0" />
							</font>
						</Label>
						<Label fx:id="label_password">
							<font>
								<Font name="Arial" size="16.0" />
							</font>
						</Label>
					</children>
				</HBox>
				<HBox alignment="TOP_CENTER" layoutX="10.0" layoutY="197.0">
					<children>
						<Label alignment="TOP_CENTER" text="Email:">
							<HBox.margin>
								<Insets bottom="10.0" right="3" />
							</HBox.margin>
							<font>
								<Font name="Arial Bold" size="16.0" />
							</font>
						</Label>
						<Label fx:id="label_email">
							<font>
								<Font name="Arial" size="16.0" />
							</font>
						</Label>
					</children>
				</HBox>
				<HBox alignment="TOP_CENTER" layoutX="10.0" layoutY="224.0">
					<children>
						<Label alignment="TOP_CENTER" text="Privilege:">
							<HBox.margin>
								<Insets bottom="10.0" right="3" />
							</HBox.margin>
							<font>
								<Font name="Arial Bold" size="16.0" />
							</font>
						</Label>
						<Label fx:id="label_privilege">
							<font>
								<Font name="Arial" size="16.0" />
							</font>
						</Label>
					</children>
				</HBox>
				<Button fx:id="button_close" mnemonicParsing="false"
					onAction="#onClosePressed" text="Close">
					<font>
						<Font name="Arial" size="16.0" />
					</font>
				</Button>
			</children>
		</VBox>
	</children>
</AnchorPane>
